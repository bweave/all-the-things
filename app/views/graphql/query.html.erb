<% @data.keys.each do |query| %>
  <div class="page-header">
    <h1><%= query.titleize %></h1>
  </div>

  <table>
    <thead>
      <tr>
        <% @data[query].first.keys.each do |heading| %> 
          <th><%= heading.titleize %></th>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% @data[query].each do |result| %>
        <%= render "#{query}_row", result: result %>
      <% end %>
  </table>
<% end %>
